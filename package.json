{
  "name": "jmlparty-api",
  "description": "API for the Jumalauta party website(s)",
  "license": "UNLICENSED",
  "author": {
    "name": "Teemu Lahti",
    "email": "teemu.lahti@gmail.com"
  },
  "scripts": {
    "compile": "tsc",
    "deploy:dev": "node_modules/.bin/sls deploy --stage dev",
    "deploy:prod": "node_modules/.bin/sls deploy --stage production",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "prepare": "husky",
    "start": "node_modules/.bin/sls dev",
    "test:integration": "vitest  -c vitest.integration.config.ts --run",
    "test:unit": "vitest  -c vitest.config.ts --run"
  },
  "dependencies": {
    "@aws-sdk/client-ssm": "3.972.0",
    "@middy/core": "7.0.2",
    "@middy/http-cors": "7.0.2",
    "@middy/http-error-handler": "7.0.2",
    "@middy/http-event-normalizer": "7.0.2",
    "@middy/http-header-normalizer": "7.0.2",
    "@middy/http-security-headers": "7.0.2",
    "date-fns": "4.1.0",
    "googleapis": "170.1.0"
  },
  "devDependencies": {
    "@eslint/compat": "2.0.1",
    "@eslint/eslintrc": "3.3.3",
    "@eslint/js": "9.39.2",
    "@types/aws-lambda": "8.10.145",
    "aws-sdk-client-mock": "4.1.0",
    "dotenv": "17.2.3",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.5",
    "eslint-plugin-unicorn": "62.0.0",
    "esnext": "3.3.1",
    "globals": "17.0.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "serverless": "4.31.0",
    "serverless-domain-manager": "8.0.0",
    "ts-node": "10.9.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.53.1",
    "vitest": "4.0.17"
  },
  "engines": {
    "node": "22.x"
  }
}
